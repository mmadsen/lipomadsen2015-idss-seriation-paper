.PHONY : clean

pdf:
	export PATH=$PATH:/usr/texbin:/usr/local/bin
	latexdiff -t CCHANGEBAR lipomadsendunnell2015-idss-seriation-original-submission.tex lipomadsendunnell2015-idss-seriation.tex > lipomadsendunnell2015-idss-seriation-revision-AUTOGENERATED.tex
	latexmk -f -pv -pdf lipomadsendunnell2015-idss-seriation-revision-AUTOGENERATED.tex

final:
	export PATH=$PATH:/usr/texbin:/usr/local/bin
	latexmk -f -pv -pdf lipomadsendunnell2015-idss-seriation-revised-submission.tex


clean:
	latexmk -CA
	rm -rf *.log *.bbl *.blg *.out *.md *.docx *.epub *.aux *.fls *.fdb_latexmk
	#rm -f lipomadsendunnell2015-idss-seriation-revision-AUTOGENERATED*
	#rm -f lipomadsendunnell2015-idss-seriation.pdf
